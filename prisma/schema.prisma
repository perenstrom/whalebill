generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_CONNECTION_STRING")
}

model Position {
  id        String @id @default(uuid())
  adminId   String @unique @default(uuid()) @map("admin_id")
  name      String
  openSeats Int    @map("open_seats")

  @@map("positions")
}
